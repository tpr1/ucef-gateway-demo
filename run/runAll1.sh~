#!/usr/bin/env bash

cd ../../
CURR_DIR=`pwd`
FEDMANAGER_HOME="$CURR_DIR/ucef-core/cpswt-core/fedmanager-exec"
DEMO_HOME="$CURR_DIR/ucef-gateway-demo"
TRIPLE_HOME="$CURR_DIR/TripleFederation_generated"
TRIPLESEND_HOME="$CURR_DIR/TripleFederation_generated/TripleFederation-java-federates/TripleFederation-impl-java/TripleSend"
TRIPLEREC_HOME="$CURR_DIR/TripleFederation_generated/TripleFederation-java-federates/TripleFederation-impl-java/TripleReceive"
cd $FEDMANAGER_HOME
echo `pwd`
./build.sh
cd "$TRIPLE_HOME"
./build.sh
cd "$TRIPLESEND_HOME"
./build.sh
cd "$TRIPLEREC_HOME"
./build.sh
cd "$DEMO_HOME"
./build.sh

